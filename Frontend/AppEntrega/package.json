{
  "name": "my-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "update:": "NODE_ENV=production pkill -f expo; expo export --experimental-bundle --clear --output-dir dist/${APP_BUILD}-${APP_ENV} && eas update --auto --branch=${BRANCH} --input-dir dist/${APP_BUILD}-${APP_ENV}"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/native": "^6.0.2",
    "axios": "^1.6.8",
    "expo": "~50.0.11",
    "expo-font": "~11.10.3",
    "expo-jwt": "^1.7.0",
    "expo-linking": "~6.2.2",
    "expo-location": "~16.5.5",
    "expo-router": "~3.4.10",
    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.4",
    "expo-updates": "~0.24.12",
    "expo-web-browser": "~12.8.2",
    "jwt-decode": "^4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-expo-swipe-button": "^1.0.2",
    "react-native-maps": "1.10.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-swipeable": "^0.6.0",
    "react-native-web": "~0.19.6",
    "react-use-websocket": "^4.8.1",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "jest": "^29.3.1",
    "jest-expo": "^50.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.0"
  },
  "private": true
}
